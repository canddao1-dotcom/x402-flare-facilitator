# FlareBank Monitoring - Environment Variables
# Copy this to .env and fill in your values

# Hypernative API (for event monitoring)
HYPERNATIVE_CLIENT_ID=your_client_id_here
HYPERNATIVE_SECRET=your_secret_here

# Flare RPC (public endpoint works, but you can use your own)
FLARE_RPC_URL=https://flare-api.flare.network/ext/C/rpc
